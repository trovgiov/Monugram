<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="default1" extends="struts-default,json-default" >

    <action name="loginback" class="it.uniclam.action.backend.Login_back">
        <result name="success">/private/backend/login_back_succ.jsp</result>
        <result name="log_error">/public/backend/login_back.jsp</result>
        <result name="input">/public/backend/login_back.jsp</result>
    </action>


        <action name="people_list" method="list" class="it.uniclam.action.backend.Controller.ControllerUser">
            <result type="json"></result>
        </action>


        <action name="users">
            <result>/private/backend/users.jsp</result>

        </action>

        <action name="home_back" class="it.uniclam.action.backend.home_back">
            <result>/private/backend/dashboard.jsp</result>

        </action>


    </package>
</struts>